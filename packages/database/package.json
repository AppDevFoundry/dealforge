{
  "name": "@dealforge/database",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./schema": {
      "types": "./src/schema/index.ts",
      "default": "./src/schema/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/seed.ts",
    "db:seed:mh-parks": "tsx src/seed-mh-parks.ts",
    "sync:ccn": "tsx src/scripts/sync-ccn-data.ts",
    "sync:flood": "tsx src/scripts/sync-flood-data.ts",
    "sync:flood:seed": "tsx src/scripts/seed-flood-data.ts",
    "sync:tdhca:titles": "tsx src/scripts/sync-tdhca-titles.ts",
    "sync:tdhca:liens": "tsx src/scripts/sync-tdhca-liens.ts",
    "discover:parks": "tsx src/scripts/discover-parks.ts",
    "calc:distress": "tsx src/scripts/calculate-distress-scores.ts",
    "clear:fake": "tsx src/scripts/clear-fake-data.ts",
    "verify:data": "tsx src/scripts/verify-data.ts",
    "seed:export": "tsx src/scripts/export-seed-data.ts",
    "seed:import": "tsx src/scripts/import-seed-data.ts",
    "load:all": "tsx src/scripts/load-all-data.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@paralleldrive/cuid2": "^3.0.6",
    "drizzle-orm": "^0.45.1",
    "pg": "^8.17.2",
    "shpjs": "^5.0.1"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.15",
    "better-auth": "^1.4.12",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0"
  }
}
