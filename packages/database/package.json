{
  "name": "@dealforge/database",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./schema": {
      "types": "./src/schema/index.ts",
      "default": "./src/schema/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/seed.ts",
    "db:seed:mh-parks": "tsx src/seed-mh-parks.ts",
    "sync:ccn": "tsx src/scripts/sync-ccn-data.ts",
    "sync:flood": "tsx src/scripts/sync-flood-data.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@paralleldrive/cuid2": "^3.0.6",
    "drizzle-orm": "^0.38.3",
    "shpjs": "^5.0.1"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.15",
    "better-auth": "^1.4.12",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.30.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
